<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/toorBar"></solid>
            <corners android:radius="@dimen/progress"></corners>
        </shape>
    </item>

    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/imgBar"></solid>
            <corners android:radius="@dimen/progress"></corners>
        </shape>
    </item>


</selector>